  "dependencies": {
    "@ant-design/pro-layout": "^6.5.0",
    "@types/chrome": "0.0.180",
    "lodash": "^4.17.21",
    "react": "17.x",
    "react-dom": "17.x",
    "ahooks": "^3.1.1",
    "classnames": "^2.3.1",
    "html-to-image": "^1.9.0",
    "immer": "^9.0.7",
    "moment": "^2.24.0",
    "monaco-editor": "^0.31.1",
    "node-fetch": "^3.2.0",
    "prop-types": "^15.5.8",
    "react-monaco-editor": "^0.46.0",
    "uuid": "^8.3.2",
    "umi": "^3.5.21"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.181",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.5.21",
    "lint-staged": "^10.0.7",
    "prettier": "^2.2.0",
    "typescript": "^4.1.2",
    "yorkie": "^2.0.0"
  }
}